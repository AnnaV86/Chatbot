{
	"compilerOptions": {
		"target": "ES5",
		"module": "ESNext",
		"moduleResolution": "node",
		"lib": ["DOM", "ESNext"],
		"jsx": "react-jsx",
		"strict": true,
		"noEmit": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"noImplicitAny": true
	},
	"include": ["src/**/*"]
}
